!             Normal        Shift       ModeSwitch    MoSw-Shift  AltGr                Shift-AltGr      MoSw-AltGr           MoSw-Shift-AltGr

keycode 13  = 4             dollar      4             dollar      yen                  dollar           yen                  dollar
keycode 21  = apostrophe    grave       apostrophe    grave       dead_acute           dead_grave       dead_acute           dead_grave
keycode 34  = bracketleft   braceleft   bracketleft   braceleft   asciicircum          dead_circumflex  asciicircum          dead_circumflex
keycode 35  = bracketright  braceright  bracketright  braceright  asciitilde           dead_diaeresis   asciitilde           dead_diaeresis
keycode 49  = at            Greek_pi    at            Greek_pi    onehalf              Greek_pi         onehalf              Greek_pi
keycode 51  = slash         asterisk    slash         asterisk    backslash            asterisk         backslash            asterisk
keycode 65  = space         space       space         space       space                space            space                space

!numpad
keycode 63  = asterisk      asciicircum asterisk      asciicircum asterisk             asciicircum      asterisk             asciicircum
keycode 79  = 7             s           7             s           7                    s                7                    s
keycode 80  = 8             parenleft   8             parenleft   8                    parenleft        8                    parenleft
keycode 81  = 9             parenright  9             parenright  9                    parenright       9                    parenright
keycode 82  = minus         minus       minus         minus       minus                minus            minus                minus
