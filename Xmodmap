keycode 85  = 6             t           6             t           6                    t                6                    t
keycode 86  = plus          plus        plus          plus        plus                 plus             plus                 plus
keycode 87  = 1             x           1             x           1                    x                1                    x
keycode 88  = 2             y           2             y           2                    y                2                    y
keycode 89  = 3             z           3             z           3                    z                3                    z
keycode 90  = 0             e           0             e           0                    e                0                    e
keycode 91  = period        comma       period        comma       period               comma            period               comma
keycode 104 = KP_Enter      equal       KP_Enter      equal       KP_Enter             equal            KP_Enter             equal

! 13 = 4
! 21 = `, next to backspace
! 34 = ao
! 35 = ~, between ao and enter
! 51 = ', between a" and enter
! 65 = space

! Clear capslock, numlock and super
clear lock
clear mod2
clear mod4

! Change Caps_Lock to super
! Win is also super
keycode 66  = Super_L
add mod4    = Super_L

